{
  "name": "easir/contacts-doublet-check",
  "type": "library",
  "description": "Doublet (double entry search) check of E2C data with EASI'R Now Contacts",
  "autoload": {
    "psr-4": {
      "Easir\\ContactsDoubletCheck\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.5",
    "nesbot/carbon": "^2.64"
  },
  "require-dev": {
    "easir/coding-standard": "@dev",
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^5.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform": {
      "php": "8.1.0",
      "ext-json": "8.1.0"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/easir/coding-standard.git"
    }
  ]
}
